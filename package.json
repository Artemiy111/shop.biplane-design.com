{
  "name": "biplane-shop",
  "private": true,
  "type": "module",
  "workspaces": ["app/shared/packages/*"],
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "bun --bun eslint .",
    "lint:fix": "bun --bun eslint . --fix",
    "lint:style": "stylelint '**/*.{css,vue}'",
    "lint:style:fix": "stylelint '**/*.{css,vue}' --fix",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push && bun tsx ./server/db/push.ts",
    "db:dropmigtaion": "drizzle-kit dropmigration",
    "db:seed": "bun tsx ./server/db/seed.ts",
    "db:drop": "bun tsx ./server/db/drop.ts",
    "db:create": "podman exec -it shopbiplane-designcom_postgres_1 psql -U postgres -c \"CREATE DATABASE biplane;\"",
    "db:destroy": "podman exec -it shopbiplane-designcom_postgres_1 psql -U postgres -c \"DROP DATABASE IF EXISTS biplane;\"",
    "db:init": "bun run db:destroy && bun run db:create && bun run db:push && bun run db:seed",
    "db:studio": "drizzle-kit studio --verbose"
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.68",
    "@nuxt/eslint": "1.9.0",
    "@nuxt/fonts": "0.11.4",
    "@nuxt/icon": "2.0.0",
    "@nuxt/image": "1.11.0",
    "@nuxt/scripts": "0.11.13",
    "@nuxt/ui": "3.3.3",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.64.1",
    "@opentelemetry/exporter-prometheus": "^0.205.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.205.0",
    "@opentelemetry/sdk-metrics": "^2.1.0",
    "@opentelemetry/sdk-node": "^0.205.0",
    "@opentelemetry/sdk-trace-node": "^2.1.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@pinia/colada": "^0.17.5",
    "@pinia/colada-nuxt": "0.2.2",
    "@pinia/nuxt": "0.11.2",
    "@tresjs/cientos": "^4.3.1",
    "@tresjs/nuxt": "4.0.0",
    "@trpc/client": "^11.5.1",
    "@trpc/server": "^11.5.1",
    "@types/amqplib": "^0.10.7",
    "@vueuse/core": "^13.9.0",
    "@vueuse/integrations": "^13.9.0",
    "@vueuse/nuxt": "^13.9.0",
    "amqplib": "^0.10.9",
    "better-auth": "^1.3.11",
    "bullmq": "^5.58.5",
    "clsx": "^2.1.1",
    "drizzle-orm": "^1.0.0-beta.1-fd5d1e8",
    "eslint": "^9.35.0",
    "events": "^3.3.0",
    "ioredis": "^5.7.0",
    "lucide-vue-next": "^0.544.0",
    "minio": "^8.0.6",
    "nodemailer": "^7.0.6",
    "nuxt": "^4.1.2",
    "pg": "^8.16.3",
    "pinia": "^3.0.3",
    "redis-semaphore": "^5.6.2",
    "sharp": "^0.34.3",
    "sortablejs": "^1.15.6",
    "tailwind-merge": "^3.3.1",
    "three": "^0.180.0",
    "trpc-nuxt": "^1.2.0",
    "typescript": "^5.9.2",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1",
    "winston": "^3.17.0",
    "zod": "^4.1.8",
    "zod-form-data": "^3.0.1"
  },
  "devDependencies": {
    "@art/vue-eslint-config": "workspace:*",
    "@types/bun": "^1.2.22",
    "@types/nodemailer": "^7.0.1",
    "@types/sharp": "^0.32.0",
    "@types/sortablejs": "^1.15.8",
    "drizzle-kit": "^1.0.0-beta.1-fd5d1e8",
    "eslint-plugin-perfectionist": "^4.15.0"
  }
}
